<view class='backgroundColor'></view>
<view class="account row-box">
  <image src="/images/avatar_1.jpg"></image>
  <view>
    <text class="nickName">测试中</text>
    <text>病号: 1000</text>
  </view>
</view>
<view class="row-box four-list clearfix">
  <view>
    <icon class="iconfont icon-xiaoxi color5"></icon>
    <text>消息</text>
  </view>
  <view>
    <icon class="iconfont icon-qianbao color2"></icon>
    <text>挂号</text>
  </view>
  <view>
    <icon class="iconfont icon-zongjie color7"></icon>
    <text>处方</text>
  </view>
  <view>
    <icon class="iconfont icon-aixin-copy color1"></icon>
    <text>送心意</text>
  </view>
</view>
<view class="row-box row-list clearfix">
  <view>
    <navigator url="../login/login">
      <icon class="iconfont icon-location"></icon>
      <text>登录界面</text>
    </navigator>
  </view>
  <view bindtap="PageErrorNot" class='notPage'>
    <icon class="iconfont icon-location"></icon>
    <text>身份证 [上传]</text>
  </view>
</view>
<view class="row-box row-list clearfix">
  <view bindtap="PageErrorNot">
    <navigator url="../family/family">
      <icon class="iconfont icon-chengyuan"></icon>
      <text>家庭成员</text>
      <icon class="iconfont icon-fangxiangyou"></icon>
    </navigator>
  </view>
  <view bindtap="PageErrorNot" class='notPage'>
    <icon class="iconfont icon-yaodian"></icon>
    <text>我的药店</text>
    <icon class="iconfont icon-fangxiangyou"></icon>
  </view>
  <view bindtap="printVersion">
    <icon class="iconfont icon-banben"></icon>
    <text>版本号: {{version}}</text>
  </view>
</view>
<msg data="{{toast}}"></msg>